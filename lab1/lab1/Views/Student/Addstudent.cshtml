@model lab1.Models.Student
<h2>Student Info</h2>

<form action="/Student/Addstudent" method="post">
    <input type="text" name="Name" class="form-control" placeholder="Name">

    <input type="text" name="ID" class="form-control" placeholder="ID">

    <input type="text" name="DOB" class="form-control" placeholder="Dob">

    <input type="text" name="Credit" class="form-control" placeholder="Credit">
    <input type="text" name="CGPA" class="form-control" placeholder="CGPA">
    <input type="text" name="Dept_id" class="form-control" placeholder="DEPT id">

    <input type="submit" class="btn btn-success" value="Add" />
</form>